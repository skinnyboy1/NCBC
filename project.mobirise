                "padding-bottom": "0.5rem",
                "padding-top": "1.5rem"
              }
            },
            "amp-img, img": {
              "object-fit": "cover"
            },
            ".card-wrapper": {
              "& when not (@transparentBg)": {
                "position": "relative"
              },
              "& when (@showBorders)": {
                "border": "1px solid @bordersColor",
                "padding": "1.5rem"
              },
              "& when (@showBorders) and not (@flexDirection)": {
                "margin-bottom": "20px"
              },
              "z-index": "1",
              "@media (max-width: 991px)": {
                "flex-wrap": "wrap"
              }
            },
            "& when not (@transparentBg)": {
              ".card-wrapper:after": {
                "content": "\"\"",
                "position": "absolute",
                "left": "0",
                "right": "0",
                "top": "0",
                "bottom": "0",
                "background-color": "@cardBg",
                "opacity": "@bgOpacity",
                "z-index": "-1"
              }
            },
            ".card-box": {
              "width": "100%",
              "& when (@showBorders)": {
                "padding": "0"
              }
            },
            "& when (@checkImages) and not (@flexDirection)": {
              ".card-img": {
                "padding": "0",
                "@media (max-width: 991px)": {
                  "width": "100%",
                  "margin-bottom": "2rem"
                },
                "@media (min-width: 992px)": {
                  "& when not (@reverseContent)": {
                    "margin-right": "2rem"
                  },
                  "& when (@reverseContent)": {
                    "margin-left": "2rem"
                  }
                }
              }
            },
