            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "& when (@showTitle) and (@showSubtitle)": {
              ".section-subtitle": {
                "margin-bottom": "20px"
              }
            },
            "& when not (@showCardsSubtitle)": {
