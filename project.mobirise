          "_name": "events4",
          "_customHTML": "<section class=\"events4\" group=\"Events\" plugins=\"amp-main,amp-date-countdown,amp-mustache\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n    <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n    <header>Color</header>\n    <input type=\"color\" title=\"Counter Text\" name=\"counterText\" value=\"#ffffff\" selected>\n    <input type=\"color\" title=\"Text Block Background\" name=\"textBg\" value=\"#ffffff\" selected>\n    <header>Icons</header>\n    <input type=\"range\" inline title=\"Icons Size\" name=\"iconsSize\" min=\"1\" max=\"3\" step=\"0.25\" value=\"2\">\n    <header>Background</header>\n    <fieldset type=\"background\" name=\"bg\">\n        <input type=\"image\" title=\"Background Image\" value=\"../_images/background4.jpg\">\n        <input type=\"color\" title=\"Background Color\" value=\"#0505aa\" selected>\n    </fieldset>\n    <select title=\"Language\" name=\"lang\">\n        <option value=\"en\" selected>English</option>\n        <option value=\"ru\">Russian</option>\n    </select>\n    <input type=\"text\" name=\"zone\" title=\"Time Zone (+01:00)\" value=\"+01:00\" required>\n    <input type=\"text\" name=\"date\" title=\"End Date (2020-01-21)\" value=\"2020-01-01\" required>\n    <input type=\"text\" name=\"time\" title=\"End Time (03:14:08)\" value=\"03:14:08\" required>\n    <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type!='color'\" checked>\n    <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"bg.type!='color' && overlay\">\n    <input type=\"range\" inline title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"bg.type!='color' && overlay\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"mbr-row\">\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-7\">\n                <div class=\"cont-wrap\">\n                    <h4 class=\"mbr-pb-6 mbr-bold6 mbr-fonts-style\" mbr-theme-style=\"display-5\">Visit Us Before The End Of The Year!</h4>\n                    <div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"amp-iconfont fa fa-calendar\"></span>\n                        </div>\n                        <p class=\"custom-card-text2 mbr-fonts-style mbr-text-i\" mbr-theme-style=\"display-4\" mbr-app-selector=\".mbr-text\">Sunday, 11:00 Am - 01:00 Pm</p>\n                        <br>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"amp-iconfont fa fa-map-marker\"></span>\n                        </div>\n                        <p class=\"custom-card-text2 primary-color-text mbr-fonts-style mbr-text-i\" mbr-theme-style=\"display-4\" mbr-app-selector=\".mbr-text\"><a href=\"https://goo.gl/maps/zJbbSppfqGy\" target=\"_blank\" class=\"text-black\">NCBC</a></p>\n                        <br>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"amp-iconfont fa fa-location-arrow\"></span>\n                        </div>\n                        <p class=\"custom-card-text2 mbr-fonts-style mbr-text-i\" mbr-theme-style=\"display-4\" mbr-app-selector=\".mbr-text\">1313 Sellers Street, Philadelphia</p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-5\">\n                <div class=\"amp-countdown-wrap\">\n                    <amp-date-countdown class=\"amp-countdown mbr-row mbr-column mobirise-loader\" end-date=\"{{date}}T{{time}}.0{{zone}}\" locale=\"{{lang}}\" layout=\"flex-item\">\n                        <div placeholder class=\"placeholder\"></div>\n                        <template type=\"amp-mustache\">\n                            <div class=\"mbr-row\">\n                                <div class=\"item-wrap\">\n                                    <span class=\"count-num\">{{d}}</span>\n                                    <span class=\"count-text\">{{days}}</span>\n                                </div>\n                                <div class=\"item-wrap\">\n                                    <span class=\"count-num\">{{h}}</span>\n                                    <span class=\"count-text\">{{hours}}</span>\n                                </div>\n                                <div class=\"item-wrap\">\n                                    <span class=\"count-num\">{{m}}</span>\n                                    <span class=\"count-text\">{{minutes}}</span>\n                                </div>\n                                <div class=\"item-wrap\">\n                                    <span class=\"count-num\">{{s}}</span>\n                                    <span class=\"count-text\">{{seconds}}</span>\n                                </div>\n                            </div>\n                        </template>\n                    </amp-date-countdown>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
